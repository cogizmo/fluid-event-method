{
  "name": "@cogizmo/fluid-triggered-method",
  "version": "0.2.1-alpha",
  "description": "Custom Element that invokes an element's method when a specified event is received.",
  "main": "src/component.js",
  "scripts": {
    "build": "npm run clean && npm run build:copy",
    "clean": "node_modules/.bin/rimraf ./build",
    "build:copy": "cpy \"**/?(*)?(.*)\" \"../build\" --cwd=src --parents && npm run build:copy2",
    "build:copy2": "cpy \"+(test|samples)/**/*?(.*)\" \"./build\" --parents && npm run build:docs",
    "build:docs": "cpy \"!(README).md\" \"./build/doc\" && npm run build:license",
    "build:license": "cpy \"+(README|LICENSE)?(.*)\" \"./build/\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cogizmo/fluid-triggered-method.git"
  },
  "keywords": [
    "web-components",
    "custom-elements",
    "javascript",
    "js",
    "vanilla-js"
  ],
  "author": "Donald Lee Atkinson <fuzzicallogic@outlook.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cogizmo/fluid-triggered-method/issues"
  },
  "homepage": "https://github.com/cogizmo/fluid-triggered-method#README.md",
  "files": [
    "/LICENSE",
    "/README.md",
    "/src/Cogizmo.js",
    "/src/Cogizmo.js"
  ],
  "devDependencies": {
    "cpy-cli": "^2.0.0",
    "rimraf": "^2.6.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@polymer/iron-ajax": "^3.0.1"
  }
}
